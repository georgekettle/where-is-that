<% if user_signed_in? %>
  <div class="w-full bg-white border-t flex items-center justify-center p-2 pb-3 fixed bottom-0 left-0" style="z-index: 999">
    <%= link_to '#', class: "mx-2 h-10 w-10 stroke-current text-gray-400 hover:text-gray-900 flex items-center justify-center rounded-full" do %>
      <%= render "icons/home", classes: "h-7 w-7" %>
    <% end %>
    <%= link_to '#', class: "mx-2 h-10 w-10 stroke-current text-gray-400 hover:text-gray-900 flex items-center justify-center rounded-full" do %>
      <%= render "icons/search", classes: "h-7 w-7" %>
    <% end %>
    <%= link_to '#', class: "mx-2 h-10 w-10 stroke-current text-gray-400 hover:text-gray-900 flex items-center justify-center rounded-full" do %>
      <%= render "icons/plus", classes: "h-7 w-7" %>
    <% end %>
    <%= link_to '#', class: "mx-2 h-10 w-10 stroke-current text-gray-400 hover:text-gray-900 flex items-center justify-center rounded-full" do %>
      <%= render "icons/profile", classes: "h-7 w-7" %>
    <% end %>
  </div>
<% end %>